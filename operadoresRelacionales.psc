Algoritmo operadoresRelacionales
	//Op. de igualdad
	a = 5 == 5;
	Imprimir 'Operador igual: ', a;
	//Op. diferente
	b = 5 <> 5;
	Escribir  'Operador diferente: ', b;
	//mayor que
	c = 6 > 5
	Escribir  'Operador Mayor que: ', c;
	//Mayor o igual que
	d = 6 >= 6;
	Escribir 'Operador mayor o igual que: ', d;
	//Menor que
	e = 7 < 7;
	Escribir 'Operador menor que: ', e;
	//Menor o igual que
	f = 8 <= 8;
	Escribir 'Operador menor o igual que: ', f;
FinAlgoritmo
